    <actionsetting notificationMessage="" name="Round to Grid depending on scale" shortTitle="Round to Grid" action="from qgis.core import QgsProject, QgsFeature, QgsGeometry, QgsPointXY, QgsCoordinateReferenceSystem, QgsCoordinateTransform&#xd;&#xa;from qgis.utils import iface&#xd;&#xa;import traceback&#xd;&#xa;&#xd;&#xa;# Define a function to calculate the grid size based on the scale&#xd;&#xa;def get_grid_size(scale):&#xd;&#xa;    try:&#xd;&#xa;        if scale >= 25000:&#xd;&#xa;            return 250&#xd;&#xa;        elif scale >= 10000:&#xd;&#xa;            return 100&#xd;&#xa;        else:&#xd;&#xa;            return 50&#xd;&#xa;    except Exception as e:&#xd;&#xa;        print(f&quot;Error determining grid size: {e}&quot;)&#xd;&#xa;        return None&#xd;&#xa;&#xd;&#xa;# Define the action to transform, round, and update the geometry for all features&#xd;&#xa;def round_to_grid_size():&#xd;&#xa;    try:&#xd;&#xa;        layer = iface.activeLayer()&#xd;&#xa;        if not layer:&#xd;&#xa;            raise Exception(&quot;No active layer selected.&quot;)&#xd;&#xa;        &#xd;&#xa;        print(&quot;Active layer selected.&quot;)&#xd;&#xa;&#xd;&#xa;        crs_src = layer.crs()  # Get the current CRS of the layer&#xd;&#xa;        crs_dst = QgsCoordinateReferenceSystem(&quot;EPSG:2056&quot;)  # Swiss coordinate system&#xd;&#xa;        &#xd;&#xa;        transform = QgsCoordinateTransform(crs_src, crs_dst, QgsProject.instance())&#xd;&#xa;&#xd;&#xa;        # Get all features from the layer&#xd;&#xa;        all_features = layer.getFeatures()&#xd;&#xa;        &#xd;&#xa;        print(f&quot;Processing {layer.featureCount()} features.&quot;)&#xd;&#xa;&#xd;&#xa;        # Start editing the layer&#xd;&#xa;        layer.startEditing()&#xd;&#xa;&#xd;&#xa;        # Loop through all features&#xd;&#xa;        for feature in all_features:&#xd;&#xa;            print(f&quot;Processing feature ID: {feature.id()}&quot;)&#xd;&#xa;&#xd;&#xa;            geom = feature.geometry()&#xd;&#xa;            if geom.isEmpty():&#xd;&#xa;                print(f&quot;Feature {feature.id()} has no geometry. Skipping.&quot;)&#xd;&#xa;                continue  # Skip empty geometries&#xd;&#xa;            &#xd;&#xa;            if geom.type() != QgsWkbTypes.PointGeometry:&#xd;&#xa;                print(f&quot;Feature {feature.id()} is not a point geometry. Skipping.&quot;)&#xd;&#xa;                continue  # Skip non-point geometries&#xd;&#xa;            &#xd;&#xa;            point = geom.asPoint()  # Assuming it's a point layer&#xd;&#xa;            print(f&quot;Original point coordinates: {point.x()}, {point.y()}&quot;)&#xd;&#xa;            &#xd;&#xa;            # Get the scale value from the attribute field&#xd;&#xa;            scale_field = &quot;Scale&quot;  # Adjust this to match your actual field name&#xd;&#xa;            if scale_field not in feature.fields().names():&#xd;&#xa;                print(f&quot;Field '{scale_field}' not found in feature {feature.id()}. Skipping.&quot;)&#xd;&#xa;                continue  # Skip if the scale field is missing&#xd;&#xa;            &#xd;&#xa;            scale = feature[scale_field]&#xd;&#xa;            print(f&quot;Scale for feature {feature.id()}: {scale}&quot;)&#xd;&#xa;            &#xd;&#xa;            grid_size = get_grid_size(scale)&#xd;&#xa;            if grid_size is None:&#xd;&#xa;                print(f&quot;Invalid grid size for feature {feature.id()}. Skipping.&quot;)&#xd;&#xa;                continue  # Skip invalid grid sizes&#xd;&#xa;            &#xd;&#xa;            print(f&quot;Grid size: {grid_size}&quot;)&#xd;&#xa;            &#xd;&#xa;            # Transform the point coordinates to EPSG:2056&#xd;&#xa;            transformed_point = transform.transform(point)&#xd;&#xa;            print(f&quot;Transformed point coordinates: {transformed_point.x()}, {transformed_point.y()}&quot;)&#xd;&#xa;&#xd;&#xa;            # Round x and y to the grid size&#xd;&#xa;            rounded_x = round(round(transformed_point.x()) / grid_size) * grid_size&#xd;&#xa;            rounded_y = round(round(transformed_point.y()) / grid_size) * grid_size&#xd;&#xa;            print(f&quot;Rounded coordinates: {rounded_x}, {rounded_y}&quot;)&#xd;&#xa;            &#xd;&#xa;            # Create the new point geometry with rounded coordinates&#xd;&#xa;            rounded_point = QgsPointXY(rounded_x, rounded_y)&#xd;&#xa;            &#xd;&#xa;            # Transform the rounded point back to the original CRS&#xd;&#xa;            reverse_transform = QgsCoordinateTransform(crs_dst, crs_src, QgsProject.instance())&#xd;&#xa;            final_point = reverse_transform.transform(rounded_point)&#xd;&#xa;            &#xd;&#xa;            print(f&quot;Final point coordinates (in original CRS): {final_point.x()}, {final_point.y()}&quot;)&#xd;&#xa;&#xd;&#xa;            # Update the feature's geometry&#xd;&#xa;            new_geom = QgsGeometry.fromPointXY(final_point)&#xd;&#xa;            layer.changeGeometry(feature.id(), new_geom)&#xd;&#xa;        &#xd;&#xa;        # Commit the changes&#xd;&#xa;        layer.commitChanges()&#xd;&#xa;        print(&quot;Geometry rounding completed for all features.&quot;)&#xd;&#xa;&#xd;&#xa;    except Exception as e:&#xd;&#xa;        # Rollback if any error occurs&#xd;&#xa;        if layer.isEditable():&#xd;&#xa;            layer.rollBack()  # Corrected method name to rollBack()&#xd;&#xa;        print(f&quot;An error occurred: {e}&quot;)&#xd;&#xa;        print(traceback.format_exc())  # Print the full traceback to the console&#xd;&#xa;&#xd;&#xa;# Run the function&#xd;&#xa;round_to_grid_size()&#xd;&#xa;" id="{f40644ee-2625-4b55-96f2-7288c58debff}" type="1" capture="1" icon="" isEnabledOnlyWhenEditable="0">
